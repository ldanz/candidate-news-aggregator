%h1 Listing users

%table
  %tr
    %th Name
    %th Zipcode
    %th Economy
    %th Healthcare
    %th Social security
    %th
    %th
    %th

  - @users.each do |user|
    %tr
      %td= user.name
      %td= user.zipcode
      %td= user.economy
      %td= user.healthcare
      %td= user.social_security
      %td= link_to 'Show', user
      %td= link_to 'Edit', edit_user_path(user)
      %td= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete

%br

= link_to 'New User', new_user_path
